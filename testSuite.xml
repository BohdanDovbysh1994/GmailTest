<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TestNG Load Testing Suite">
    <listeners>
        <listener class-name="testdata.TestNgListener"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="utils.CustomListener"/>
        <listener class-name="utils.AnnotationTransformer"/>
    </listeners>
    <test name="GmailTest" parallel="instances">
        <classes>
            <class name="FactoryTest"/>
            <class name="FailedTest"/>
        </classes>
    </test>
</suite>